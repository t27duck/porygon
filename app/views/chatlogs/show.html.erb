<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Nick</th>
      <th>Time</th>
      <th>Message</th>
    </tr>
  </thead>
  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.nick %></td>
        <td><%= log.created_at %></td>
        <td><%= log.message %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @logs %>
